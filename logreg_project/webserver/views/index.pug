extends layout

block content
  h1(class='text-3xl font-bold tracking-tight text-gray-900')= title
  p Welcome to #{title}
  each feature in features
    p #{feature}
    input(type="number" step=0.01 class="feature_value")
  button(id="post_feature_values", type="button", value="Submit", onclick="postFeatureValues()") Submit
  
